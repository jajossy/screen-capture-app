{
  "name": "react-webpack-babel",
  "version": "1.0.0",
  "description": "React webpack with babel",
  "main": "./app/build/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently \"cross-env NODE_ENV=development webpack-dev-server --config webpack/dev.config.js\" \"cross-env NODE_ENV=development electron ./app/src/main/app.js\"",
    "prod": "cross-env NODE_ENV=production webpack --progress --color --config webpack/prod.config.js",
    "start": "npm run prod && electron app/build/app.js",
    "pack": "electron-builder --dir",
    "dist": "npm run prod && electron-builder",
    "server": "node ./server/server.js",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "creative-tim",
    "material-design"
  ],
  "author": "Jolaosho Joseph",
  "license": "ISC",
  "build": {
    "productName": "Snipping Tool - Farouk and Joseph",
    "win": {
      "target": "NSIS"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "directories": {
      "output": "release"
    }
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/node": "7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "concurrently": "^4.1.2",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.3",
    "css-loader": "3.2.0",
    "electron": "^9.4.0",
    "electron-builder": "^21.2.0",
    "electron-devtools-installer": "^2.2.4",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "4.12.0",
    "path": "0.12.7",
    "react-hot-loader": "^4.12.14",
    "sass-loader": "8.0.0",
    "style-loader": "1.0.0",
    "webpack": "4.41.0",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.8.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "express": "^4.17.1",
    "jimp": "^0.8.5",
    "jquery": "^3.4.1",
    "popper.js": "^1.16.0",
    "react": "16.10.1",
    "react-dom": "16.10.1",
    "react-electron-web-view": "^2.0.1",
    "react-electron-webview": "^2.0.2",
    "react-rnd": "^10.1.1",
    "react-spinners": "^0.6.1",
    "spectre.css": "^0.5.8",
    "uuid": "^3.3.3"
  }
}
